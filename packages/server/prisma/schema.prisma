generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("LOCAL_POSTGRES_URL")
}

model User {
  id       String  @id @default(cuid())
  sid      String?  @unique
  address  String  @unique
  nonce    String  @unique
  username String? @unique
}

model Session {
  id        String   @id
  sid       String   @unique
  data      String
  expiresAt DateTime
}
