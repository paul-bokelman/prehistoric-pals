{
  "name": "server",
  "version": "2.0.0",
  "author": "Paul Bokelman <paul.bokelman1@gmail.com>",
  "description": "Demo project for 'One-click Login with Blockchain: A MetaMask Tutorial'",
  "license": "MIT",
  "private": true,
  "scripts": {
    "server:build": "tsc && tsc-alias",
    "server:start": "node build/app.js",
    "server:dev": "nodemon --watch './**/*.ts' --exec 'ts-node -r tsconfig-paths/register' ./src/app.ts",
    "server:dev:watch": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\" \"nodemon -q build/app.js\""
  },
  "prisma": {
    "schema": "./prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "@quixo3/prisma-session-store": "^3.1.5",
    "@types/cors": "^2.8.8",
    "aws-sdk": "^2.1136.0",
    "body-parser": "^1.18.3",
    "color-convert": "^2.0.1",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "delta-e": "^0.0.8",
    "ethereumjs-util": "^7.1.4",
    "ethers": "^5.6.6",
    "express": "^4.18.1",
    "express-jwt": "^6.1.2",
    "jimp": "^0.16.1",
    "jsonwebtoken": "^8.5.1",
    "nanoid": "^3.3.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "replace-multiple-color": "^1.0.0"
  },
  "devDependencies": {
    "@types/color-convert": "^2.0.0",
    "@types/cookie": "^0.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/delta-e": "^0.0.0",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.4",
    "@types/express-session": "^1.17.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.33",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "axios": "^0.27.2",
    "concurrently": "^7.2.0",
    "dotenv": "^16.0.1",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "tsc-alias": "^1.6.7",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  }
}
